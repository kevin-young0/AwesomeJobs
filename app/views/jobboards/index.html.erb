<h1>Listing jobboards</h1>

<table>
  <thead>
    <tr>
      <th>Jobtitle</th>
      <th>Company</th>
      <th>Hours</th>
      <th>Full</th>
      <th>Parttime</th>
      <th>Salary</th>
      <th>Description</th>
      <th>Requirements</th>
      <th>Website</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @jobboards.each do |jobboard| %>
      <tr>
        <td><%= jobboard.jobTitle %></td>
        <td><%= jobboard.company %></td>
        <td><%= jobboard.hours %></td>
        <td><%= jobboard.full %></td>
        <td><%= jobboard.partTime %></td>
        <td><%= jobboard.salary %></td>
        <td><%= jobboard.description %></td>
        <td><%= jobboard.requirements %></td>
        <td><%= jobboard.website %></td>
        <td><%= link_to 'Show', jobboard %></td>
        <td><%= link_to 'Edit', edit_jobboard_path(jobboard) %></td>
        <td><%= link_to 'Destroy', jobboard, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jobboard', new_jobboard_path %>
